	"script": {
		"source": [
			"double*(",
				"w1 = 0.5L / (1.0L - 0.125L)",
				"w2 = 0.25L / (1.0L - 0.125L)",
				"w3 = 0.125L / (1.0L - 0.125L)",
			")",
			"(",
				"+ 512.0L * computeOverworldSlope(",
					"creased_hilliness,",
					"`heightmap/4096` * w1 + `heightmap/2048` * w2 + `heightmap/1024` * w3,",
					"`amp/4096` * w1 + `amp/2048` * w2 + `amp/1024` * w3,",
					"dz_creased_hilliness,",
					"`dz_heightmap/4096` * w1 + `dz_heightmap/2048` * w2 + `dz_heightmap/1024` * w3,",
					"`dz_amp/4096` * w1 + `dz_amp/2048` * w2 + `dz_amp/1024` * w3",
				")",
