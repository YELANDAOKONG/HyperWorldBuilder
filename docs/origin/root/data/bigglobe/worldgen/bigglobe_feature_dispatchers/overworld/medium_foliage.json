{
	"type": "script",
	"dispatcher": [
		"for (",
			"int z in range[minModifiableZ, maxModifiableZ],",
			"int x in range[minModifiableX, maxModifiableX]",
		":",
			"int y = world_traits.`bigglobe:y_level_on_surface`(x, z)",
			"Biome biome = world_traits.`bigglobe:biome`(x, y, z)",
			"if (biome.isIn('#bigglobe:ocean'):",
				"random.if (0.0625L / (fastExp(-4.0L * world_traits.`bigglobe:foliage_at_surface`(x, z)) + 1.0L):",
					"placeFeature(x, y, z, 'bigglobe:overworld/kelp')",
				")",
			")",
			"else if (biome.isIn('#bigglobe:hot'):",
				"random.if (",
					"(0.015625L / (fastExp(-4.0L * world_traits.`bigglobe:foliage_at_surface`(x, z)) + 1.0L))",
					"* (1.0I - 0.25I / (world_traits.`bigglobe:magicalness`(x, z) ^ 2 + 0.25I))",
				":",
					"placeFeature(x, y, z, 'bigglobe:overworld/cactus')",
				")",
			")",
			"else if (biome.isIn('bigglobe:temperate_dense_forest', 'bigglobe:warm_dense_forest'):",
				"random.if (0.125L:",
					"placeFeature(x, y, z, 'bigglobe:overworld/bamboo')",
				")",
			")",
		")"
	]
}